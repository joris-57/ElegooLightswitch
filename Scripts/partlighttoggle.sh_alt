#!/bin/bash
GPIO_PIN=79
CURRENT_VALUE=$(cat /sys/devices/platform/pinctrl/ff230000.gpio/gpiochip2/gpio/gpio$GPIO_PIN/active_low)
# Flip the GPIO pin
if [ $CURRENT_VALUE -eq 1 ]
then
    echo 0 | tee /sys/devices/platform/pinctrl/ff230000.gpio/gpiochip2/gpio/gpio$GPIO_PIN/active_low > /dev/null
else
    echo 1 | tee /sys/devices/platform/pinctrl/ff230000.gpio/gpiochip2/gpio/gpio$GPIO_PIN/active_low > /dev/null
fi
